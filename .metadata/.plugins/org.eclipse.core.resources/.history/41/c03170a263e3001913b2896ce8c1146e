package test2;

public class Student {
	private String name;
	private int score;
	private int grade;
	
	public Student(String n, int score, int grade) {
		this.score = score;
		this.name = n;
		this.grade = grade;
		System.out.println("Successfully initialized");
	}
	
	public void getInfo() {
		System.out.println("name: " + name);
		System.out.println("score: " + score);
		System.out.println("grade: " + grade);
	}
	
}
